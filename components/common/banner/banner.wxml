<!--components/common/banner/banner.wxml-->
<swiper indicator-dots="{{indicatorDots}}" indicator-active-color="#fff"
   interval="{{interval}}"  duration="{{duration}}" autoplay="true" class="swiper">
  <block wx:for="{{imgUrls}}" wx:key="unique">
    <swiper-item>
      <image src="{{item.banner}}" mode='widthFix' class="slide-image"  bindtap='jump' data-info="{{item}}" />
    </swiper-item>
  </block>
</swiper>
