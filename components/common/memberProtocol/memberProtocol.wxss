/* component/popup.wxss */

.member-protocol {
  position: fixed;
  left: 0;
  top: 0;
  width: 100%;
  height: 100%;
  background: rgba(0, 0, 0, 0.6);
  z-index: 999;
}

.mask {
  position: fixed;
  left: 0;
  top: 0;
  width: 100%;
  height: 100%;
  opacity: 0;
  z-index: 99;
}

.protocol-container {
  position: absolute;
  width: 80%;
  min-height: 50%;
  max-width: 100%;
  overflow: hidden;
  background: #fff;
  border-radius: 24rpx 24rpx 0 0;
  z-index: 100;
}

.protocol-container.center {
  left: 50%;
  top: 50%;
  border-radius: 24rpx;
  transform: translate(-50%, -50%);
}

.protocol-container.footer {
  left: 0;
  bottom: 0;
}

