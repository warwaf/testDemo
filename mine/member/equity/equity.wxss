.body,
.equity,
.head,
.line-case,
.list {
  box-sizing: border-box
}
page {
  background: #28292F;
}
.equity {
  background: #28292F;
  padding: 48rpx 30rpx 40rpx;
  margin-top: 88rpx;
}

.equity .module {
  margin-bottom: 60rpx
}

.equity .head {
  width: 130px;
  height: 32px;
  display: flex;
  flex-flow: row nowrap;
  justify-content: center;
  align-items: center;
  position: relative;
  margin: 0 auto
}

.equity .head .dot {
  display: flex;
  border-radius: 50%;
  background: linear-gradient(270deg, rgba(252, 237, 189, 0) 0%, rgba(252, 237, 189, 0.25));

}

.equity .head .left-dot {
  width: 8px;
  height: 8px;
  position: absolute;
  top: 0;
  left: -10px
}

.equity .head .right-dot {
  width: 12px;
  height: 12px;
  position: absolute;
  top: -6px;
  right: -14px
}

.equity .head .title {
  width: 272rpx;
  height: 60rpx;
  margin: 0;
  font-size: 16px;
  color: #FCEDBD;
  font-weight: 400;
  line-height: 60rpx;
  text-align: center;
  border-radius: 30rpx;
  background: linear-gradient(270deg, rgba(252, 237, 189, 0.04) 15%, rgba(252, 237, 189, 0.25) 90%);
}

.equity .body {
  width: 100%;
  margin-top: 40rpx;
  background: #fff;
  border-radius: 6px;
  padding: 22px 0 22px 18px
}

.equity .body .list {
  width: 100%;
  /* height: 30px; */
  margin-bottom: 30rpx;
  display: flex;
  flex-flow: row nowrap;
  justify-content: flex-start;
  align-items: center;
  padding-right: 10px
}

.equity .body .list:last-child {
  margin-bottom: 0;
}

.equity .body .list .line-case {
  position: relative;
  width: 30px;
  height: 30px;
  display: flex;
  justify-content: center;
  align-items: center
}

.equity .body .list .line-case image {
  width: inherit;
  height: inherit
}

.equity .body .list .line-case text {
  font-size: 16px;
  color: #bc6600;
  line-height: 16px;
  font-weight: 700;
  position: absolute
}

.equity .body .list .text {
  color: #bc6600;
  font-size: 13px;
  margin-left: 16px
}

.equity .body .list-color1 .text {
  color: #222;
  margin-left: 0;
  font-size: 14px;
}

.equity .tip {
  font-size: 12px;
  color: #B3B3B3;
  margin-top: 8rpx;
  text-align: center;
}