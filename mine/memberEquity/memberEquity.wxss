.body,.equity,.head,.line-case,.list{box-sizing:border-box}.equity{background:#fffaed;padding:16px 15px 17px}.equity .module{margin-bottom:20px}.equity .head{width:130px;height:32px;display:flex;flex-flow:row nowrap;justify-content:center;align-items:center;position:relative;margin:0 auto}.equity .head .dot{display:flex;border-radius:50%;background:linear-gradient(#fff1c3,#ffc205)}.equity .head .left-dot{width:8px;height:8px;position:absolute;top:0;left:-10px}.equity .head .right-dot{width:12px;height:12px;position:absolute;top:-6px;right:-14px}.equity .head .title{width:130px;height:32px;margin:0;font-size:16px;color:#bc6600;font-weight:700;line-height:32px;text-align:center;border-radius:100px;background:linear-gradient(to right,#ffeec3,#ffe086)}.equity .body{width:100%;margin-top:14px;background:#fff;border-radius:6px;padding:22px 0 22px 18px}.equity .body .list{width:100%;height:30px;margin-bottom:29px;display:flex;flex-flow:row nowrap;justify-content:flex-start;align-items:center;padding-right:10px}.equity .body .list .line-case{position:relative;width:30px;height:30px;display:flex;justify-content:center;align-items:center}.equity .body .list .line-case image{width:inherit;height:inherit}.equity .body .list .line-case text{font-size:16px;color:#bc6600;line-height:16px;font-weight:700;position:absolute}.equity .body .list .text{color:#bc6600;font-size:13px;margin-left:16px}