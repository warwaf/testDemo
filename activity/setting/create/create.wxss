page{background-color:#f9fafc}.activity-item{width:90%;margin:10px auto;border-radius:10px;background-color:#fff}.activity-item .top{padding:0 15px;display:flex;justify-content:space-between;align-items:center;height:50px;border-bottom:1px solid #ededed}.activity-item .top image{width:2px;height:16px;margin-right:5px;position:relative;top:3px}.activity-item .top view{overflow:hidden;text-overflow:ellipsis;white-space:nowrap}.activity-item .body>view{padding:10px}.activity-item .body .icon{width:14px;height:14px}.activity-item .body .prize-item{display:flex;height:48px;align-items:center}.activity-item .body .prize-item image{width:26px;height:26px;margin-right:20px}.activity-item .body .prize-item .rule-info>view:nth-child(1){font-weight:700}.activity-item .body .prize-item .rule-info>view:nth-child(2){font-size:13px;color:#666;margin-top:5px}.activity-item .body .prompt{border-top:1px solid #ededed;padding:20px 10px}.activity-item .body .prompt .icon{width:18px;height:18px;margin-right:5px}.activity-item .body .prompt .icon text{font-weight:700}.empty{display:flex;flex-direction:column;align-items:center;justify-content:center;margin-top:130px}.empty>view:nth-child(2){margin:30px 0 10px 0;color:#240101;font-size:16px}.empty>view:nth-child(3){color:#999}.empty image{width:163px;height:105px}.empty .empty-btn{color:#240101;background:#ffe227;border-radius:20px;margin-top:20px;width:204px;height:40px;line-height:40px;box-shadow:rgba(255,226,39,.71);text-align:center}.session{width:100vw;background-color:#fff;height:158px;position:fixed;bottom:0}.session>view:nth-child(1){display:flex;padding:0 20px;height:50px;justify-content:space-between;align-items:center}.session>view:nth-child(1)>text:nth-child(1){color:#666}.session>view:nth-child(1)>text:nth-child(2){color:#ffb800}.session>view:nth-child(2){font-size:17px;color:#222;font-weight:700;margin:10px 0;text-align:center}.session>view:nth-child(3){color:#666;text-align:center}