page{background-color:#f9fafc}.record .title{display:inline-block;padding:0 15px 0 25px;height:23px;line-height:23px;border-radius:11px;background-color:#ffe329;font-size:13px;color:#231815;margin-top:10px;position:relative;max-width:150px;overflow:hidden;text-overflow:ellipsis;white-space:nowrap}.record .title .circle{display:inline-block;width:2px;height:2px;border-radius:1px;background-color:#fff;position:absolute;left:10px;top:11px}.record .title .rect{display:inline-block;width:2px;height:20px;position:absolute;background-color:#ffe329;left:10px;bottom:-10px}.record .content{width:82%;margin:10px auto;background-color:#fff;border-radius:10px;padding:15px 0}.record .content .inline{text-overflow:ellipsis;overflow:hidden;white-space:nowrap;width:100%}.record .content>view{font-size:14px;color:#231815;padding:5px 10px}.record .content>view image{width:12px;height:12px;margin-right:10px}.record .content .line{height:1px;background-color:#e7e9ed;padding:0!important;margin:10px 0}.record .content .table{padding:0!important;margin:10px auto 0 auto;width:90%;border:1px solid #e7e9ed;border-bottom:none}.record .content .table .row{height:44px;border-bottom:1px solid #e7e9ed}.record .content .table .row .item{width:33%;height:44px;line-height:44px;display:block;overflow:hidden;text-overflow:ellipsis;white-space:nowrap;text-align:center;float:left;border-left:1px solid #e7e9ed}.record .content .table .row .item image{position:relative;left:0;top:2px}.record .content .table .row .item:nth-child(1){border-left:none;font-weight:700}.record .content .btn{padding:0;width:120px;text-align:center;height:26px;line-height:26px;background-color:#ffe329;border-radius:13px;margin:10px auto 0 auto}.empty{display:flex;flex-direction:column;align-items:center;justify-content:center;margin-top:130px}.empty>view:nth-child(2){margin:30px 0 10px 0;color:#240101;font-size:16px}.empty>view:nth-child(3){color:#999;margin-bottom:10px}.empty image{width:163px;height:105px}.empty .empty-btn{color