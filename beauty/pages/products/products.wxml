<view class="empty" wx:if="{{isEmpty}}">
    <image src="https://hcmtq.oss-accelerate.aliyuncs.com/resources/beauty-empty.png" mode="widthFix" />
    <view>空空如也～</view>
</view>

<view class="back-btn" catchtap="backRoom" wx:if="{{fromShare}}">
    <image src="http://hcmtq.oss-cn-hangzhou.aliyuncs.com/resource2020/v1.4/icon16.png" mode="widthFix" />
</view>