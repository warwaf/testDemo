.check-mask{
    width:100vw;
    height:100vh;
    background-color:rgba(0,0,0,.5);
    position:fixed;
    top:0;
    left:0;
    z-index:4;
    overflow: hidden;
}
.check-inner{
    width:276px;
    height:285px;
    background-color:#fff;
    margin:150px auto 0 auto;
    border-radius:10px;
    position:relative;
    z-index:5
}
.check-inner .check-title{
    text-align: center;
    padding: 20px;
}
.check-inner .select-type{
    width: 80%;
    height: 40px;
    margin:0 auto;
    border:1px solid #ffe22b;
    text-align: center;
    border-radius: 20px;
    line-height: 40px;
    margin-bottom: 20px;
}

.check-inner .open-sy{
    width: 80%;
    height: 40px;
    margin:0 auto;
    border:1px solid #fb8578;
    text-align: center;
    color: #fff;
    background-color: #fb8578;
    border-radius: 20px;
    line-height: 40px;
    margin-top: 40px;
}
.check-inner .activeype{
    background-color: #ffe22b;
}

.check-close-btn{
    position:absolute;
    bottom:-50px;
    left:calc(50% - 24px);
    width:48px;
    height:48px
}

