.search-store-container{width:345px;margin:-10px auto 15px auto;padding-bottom:50px}.search-store-container .search-store-list{height:98px;position:relative;display:flex;justify-content:space-between;align-items:center;margin:13px 0}.search-store-container .search-store-list .img{width:144px;height:72px;border-radius:10px;position:relative}.search-store-container .search-store-list .img .mask{width:144px;height:72px;background-color:rgba(0,0,0,.3);position:absolute;top:0}.search-store-container .search-store-list .icon{width:17px;height:18px;position:absolute;right:10px;top:18px}.search-store-container .search-store-list .detail{height:calc(100% - 30px);padding:15px 0;display:flex;flex-direction:column;justify-content:space}.search-store-container .search-store-list .detail>view:nth-child(1){font-size:16px;color:#222;font-weight:700;width:144px;overflow:hidden;white-space:nowrap;text-overflow:ellipsis}.search-store-container .search-store-list .dis{color:#4a4a4a;font-size:12px;margin-top:8px}.search-store-container .search-store-list .dis image{width:9px;height:11px}.search-store-container .search-store-list .views{font-size:10px;color:#9b9b9b;display:flex;align-items:center;margin-top:10px}.search-store-container .search-store-list .views view{margin-right:10px;display:flex;align-items:center}.search-store-container .search-store-list .views image{width:13px;height:11px;float:left;margin-right:5px;border-radius:0}.empty{display:flex;flex-direction:column;align-items:center}.empty image{width:163px;height:105px;margin:40px 0 20px 0}.empty view{font-size:16px;color:#240101}