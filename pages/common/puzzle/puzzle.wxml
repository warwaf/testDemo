<view class="wrapper"  >
<!-- {{!isShow ? 'active': ''}} " -->
  <view class="imgwrapp ">
    <image class="hc-img"  src="{{imgsrc}}"  mode="widthFix"></image>
  </view>
  <canvas class="wrapper-canvas"  canvas-id="image" style="width: 375px; height: 667px;"></canvas>
  <!-- <view class="Tips" style="margin-bottom: 40px;"></view> -->
  <!-- wx:if="{{isShow}}" -->
  <view class="footer_box" >
    <view class='footer_left'>
        <image src="http://hcmtq.oss-cn-hangzhou.aliyuncs.com/resources/new/share-home@2x.png" bindtap='home'></image>
    </view>
    <view class='footer_right'>
        <!-- <image src="https://hcmtq.oss-accelerate.aliyuncs.com/resources/share-big.png" bindtap='big'></image> -->
        <image src="https://hcmtq.oss-accelerate.aliyuncs.com/resources/share-upload.png" bindtap="savePhoto" ></image>
        <button open-type="share"  class='hc-btn' plain="true" style='background-image:url("data:image/png;base64,")'></button>
    </view>
  </view>
  <loading type="1" pos="top" wx:if="{{isLoading}}"></loading>
</view>