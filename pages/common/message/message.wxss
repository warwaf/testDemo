page{width:100%;height:100%}.v-content{background:#f9fafc;width:100%;height:100%;overflow-y:auto;overflow-x:hidden;padding-bottom:20px}.v-content .v-message{margin:15px 15px 0 15px}.v-content .v-message-date{font-size:13px;font-family:PingFangSC-Regular;font-weight:400;color:#999;text-align:center}.v-content .v-message-body{margin-top:10px;background:#fff;box-shadow:0 0 10px 0 #f1f3f8;border-radius:10px}.v-content .v-message-title{font-size:15px;font-family:PingFangSC-Medium;font-weight:500;color:#231815;position:relative;padding:10px 15px;padding-bottom:8px}.v-content .v-message-title-extend{position:absolute;right:15px;top:10px;font-size:13px;font-family:PingFang-SC-Regular;font-weight:400;color:#999}.v-content .v-message-content{font-size:14px;font-family:PingFang-SC-Regular;font-weight:400;color:#999;padding-bottom:8px;padding:10px 15px}.v-content .v-message-detail{padding:10px 15px;border-top:1px solid #ededed;font-size:13px;font-family:PingFang-SC-Regular;font-weight:400;color:#999;padding-top:8px;position:relative}.v-content .v-message-detail-arrow{position:absolute;right:10px;top:12px;width:15px;height:15px}.v-content .v-message-detail-arrow .right-arrow1,.v-content .v-message-detail-arrow .right-arrow2{width:0;height:0;display:block;position:absolute;left:0;top:0;border-top:6px transparent dashed;border-right:6px transparent dashed;border-bottom:6px transparent dashed;border-left:6px #fff solid;overflow:hidden}.v-content .v-message-detail-arrow .right-arrow1{left:1px;border-left:6px #999 solid}.v-content .v-message-detail-arrow .right-arrow2{border-left:6px #fff solid}.empty{position:fixed;top:0;width:100vw;height:100vh;display:flex;flex-direction:column;justify-content:center;align-items:center}.empty image{width:120px;margin-top:10px}