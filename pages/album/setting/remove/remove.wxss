.header{width:calc(100vw - 30px);padding:15px 15px 0 15px;display:flex;justify-content:space-between;align-items:center;color:#240101;font-size:16px}.header .input-container{width:73vw;height:35px;line-height:35px;background-color:#f1f3f5;border-radius:17px;padding:0 17px;display:flex;align-items:center}.header .input-container image{width:13px;height:13px}.header input{width:63vw;padding:0 5px;margin-right:5px}.header .placeholder{font-size:13px}.body{padding:0 15px 50px 15px}.body .item{display:flex;justify-content:space-between;align-items:center;border-bottom:1px solid #dee1e9;box-shadow:0 1px 1px #f1f3f8;height:70px}.body .item>view:nth-child(1){display:flex;align-items:center}.body .item image{width:40px;height:40px;border-radius:50%;margin-right:15px}.body .item .circle{width:20px;height:20px;border:1px solid #dbdbdb;border-radius:50%;display:flex;align-items:center;justify-content:center}.body .item .circle image{width:14px;border-radius:none;margin:0}.body .item .picked{background-color:#ffe227!important;border:none}.body .empty{margin-top:100px;display:flex;flex-direction:column;align-items:center}.body .empty view{margin:10px}.body .empty image{width:170px}.btn{position:fixed;bottom:12px;width:92vw;left:4vw;height:40px;line-height:40px;text-align:center;border-radius:20px}.active{background:linear-gradient(#ffe74f,#ffe227);color:#240101}.inactive{background-color:#bdbdbd;color:#fff}.mask{width:100vw;height:100vh;background-color:rgba(0,0,0,.5);position:fixed;top:0;left:0;z-index:1}.session{position:fixed;bottom:0;z-index:2;background-color:#fff;width:100vw}.session .top{display:flex;justify-content:space-between;padding:15px}.session .top>view:nth-child(1){color:#999}.session .top>view:nth-child(2){color:#ffb800}.session .bottom{height:90px;padding:0 0 30px 0;width:100%;display:flex;align-items:center;justify-content:center;flex-direction:column}.session .bottom>view:nth-child(1){color:#222;margin-bottom:10px;font-size:17px}.session .bottom>view:nth-child