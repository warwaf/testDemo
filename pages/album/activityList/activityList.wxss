}

.home {
  width: 95px;
  height: 60px;
  display: flex;
  align-items: center;
  justify-content: center;
  position: fixed;
  left: 0;
  top: 25vh;
  z-index: 1;
  border-radius: 0 17px 17px 0
}

.home image {
  width: 100%;
  height: 100%
}

.calendar-item {
  display: flex;
  justify-content: space-around;
  margin-top: 13px;
  padding: 0 5px;
  font-weight: 700
}

.calendar-item .date .d {
  color: #a7abaf;
  font-size: 10px
}

.calendar-item .date .t {
  color: #231815;
  font-size: 12px;
