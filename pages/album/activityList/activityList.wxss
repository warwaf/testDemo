}

.menu-container .tab-btn{
  flex: 1 0 0;
  overflow-x: auto;
  padding-top: 5px;
}

.menu-container .tab-btn .item{
  display: inline-block;
  text-align: center;
  width: 90px;
  background: rgba(235,235,235,1);
