  height: 20px;
  line-height: 20px
}

.calendar-item .photos {
  display: flex;
  width: 260px
}

.calendar-item .photos image {
  width: 80px;
  height: 80px;
  margin-right: 10px
}

.calendar-item>view:nth-child(3) {
  width: 9px;
  height: 100%
}

.calendar-item>view:nth-child(3) image {
  width: 9px;
  height: 15px;
  margin-top: 30px
}

.group-container {
  width: 95%;
  padding: 0 2.5%;
  display: flex;
  justify-content: space-between;
  align-items: center;
  border-bottom: 1px solid #ededed;
  background-color: #fff;
  position: sticky;
  top: 0;
  z-index: 2
}

.group-container .scroll-container {
  width: 320px;
  height: 32px;
  padding: 0 10px
}

.group-container .scroll-container .group-item {
  float: left;
  margin: 0 2px;
  width: 72px;
  height: 32px;
  line-height: 32px;
  text-align: center;
  background-color: #f2f4f6;
  border-radius: 4px 4px 0 0;
  color: #5b6b73;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
  font-size: 12px
}

.group-container .icon {
  width: 50px;
  height: 32px;
  display: flex;
  justify-content: center;
  align-items: center
}

.group-container .icon image {
  width: 12px;
  height: 12px
}

.active-0 {
  color: #fff !important;
  background: linear-gradient(-90deg, #fe5a74, #ff7a8f)
}

.active-1 {
  color: #fff !important;
  background: linear-gradient(-90deg, #ffaf18, #ffcd39)
}

.active-2 {
  color: #fff !important;
  background: linear-gradient(-90deg, #5a78fe, #af6cfb)
}

.active-3 {
  color: #fff !important;
  background: linear-gradient(-90deg, #00b1f7, #6ce2fb)
}

.group-layout {
  position: fixed;
  z-index: 7;
  top: 0;
  left: 0;
  width: 100vw;
  height: calc(100vh - 30px);
  background-color: #fff;
  padding: 15px 0
}

.group-layout .group-item {
  width: 326px;
  height: 80px;
  margin: 10px auto;
  position: relative;
  border-radius: 10px
}

.group-layout .group-item .p1 {
  padding: 10px 20px;
  font-size: 18px;
  font-weight: 500
}

.group-layout .group-item .p2 {
  padding: 0 20px;
  font-size: 12px
}

.group-layout .group-item .img {
  width: 73px;
  height: 61px;
  position: absolute;
  right: 0;
  bottom: 0
}

.menu-container {
  width: calc(100vw - 30px);
  display: flex;
  padding: 0 15px;
  justify-content: space-between;
  border-bottom: 1px solid #eee;
  background-color: #fff;
  border-radius: 15px 15px 0 0;
  position: sticky;
  z-index: 1;
  height: 40px;
}

.menu-container .icons {
  /* width: 80px; */
  justify-content: flex-end;
  /* //position: sticky; */
  padding-top: 10px;
  
}

.menu-container .icons image {
  width: 21px;
  height: 21px;
  margin-left: 15px
}

