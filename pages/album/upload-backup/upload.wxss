.box{
  position: fixed;  
  top:10rpx;
  left: 20rpx;
  height: 60rpx; 
  overflow: hidden;  
  background:linear-gradient(90deg,rgba(240,13,28,1) 0%,rgba(255,124,37,1) 100%);
  border-radius: 60rpx;
  padding: 0rpx 20rpx;
  line-height: 60rpx;
  width:346rpx;
  font-size: 30rpx; 
  text-align: center;    
}
.box swiper{
  height: 100%;
}
.txt{
  white-space: nowrap;
  user-select: none;  
  display:-webkit-box;
	display:-webkit-flex;
	display:flex; 
  flex-wrap:nowrap;
  flex-direction: row;
  justify-content: center;  
