#txt2,.flex-box{
  position: absolute;
  top: 0;
}

.flex-box{
  display: flex;
  flex-wrap: nowrap;
  justify-content: flex-start;
}
.uploadEnd{
  height: auto;
  background:linear-gradient(180deg,rgba(255,235,77,1) 0%,rgba(255,247,163,1) 100%);
  height: 250rpx;
  overflow: hidden;
  position: relative;
}
.uploadRact{ 
position:absolute;
border-top-left-radius:80%;
border-top-right-radius:80%;
border-bottom-right-radius:0%;
border-bottom-left-radius:0%;
transform:translateX(-10%);
background-color:#fff;
width:125%;
top:120rpx;
height:100rpx;

}
.uploadEndtxt{
   width: 100%;
font-size:36rpx;
font-weight:400;
color:rgba(34,34,34,1);
line-height:50rpx;
margin: 0 auto;
position: absolute;
top:170rpx;
text-align: center;
z-index: 50;
bottom: 0px;
background-color: #fff;
}
.loading{
  position: fixed;
  z-index: 3000;
  top: 0;
  bottom: 0;
  left: 0;
  right: 0;
  background:rgba(0, 0, 0, .5)
}
@keyframes roll {
  0% {left: 750rpx;}
  100% {left: -100%;}
}
.loadingGif{
  width: 182rpx;
  height: auto;
  margin: 0 auto;
  position: absolute;
  top: 50%;
  left: 50%;
 transform:translate(-50%,-50%);
 -webkit-transform: translate(-50%,-50%);
  
}
.loadingGif image{
  width: 100%;
  height: 160rpx;
}

.loadtxt{
  text-align: center;  
}
.uploadprogress{
  position: absolute;
  top: 60%;
  left: 50%;
 transform:translate(-50%,-60%);
  -webkit-transform: translate(-50%,-60%);
 border-radius: 12rpx;
}
.uploadprogress{
    z-index:100;   
    width: 232rpx;
    height: 12rpx;
}
.upload_cemera{
  width: 138rpx;
  height:auto;
  margin: 16rpx auto 16rpx auto;
  z-index: 100;
  position: absolute;
  left: 0;
  right: 0;

}
.upload_cemera image{
  width: 100%;
  height: auto;
}
.vouchers{
    position: fixed;
    z-index: 501;
}
.protocol{
    display: flex;
    /* justify-content: center; */
    height: 25px;
    text-align: left;
    margin-left: 30rpx;
    /* align-items: center; */
