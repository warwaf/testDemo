.list image{width:100%;margin-bottom:10px}.mask{width:100vw;height:100vh;background-color:rgba(0,0,0,.5);position:fixed;top:0;left:0}.mask .inner{width:300px;height:245px;padding:20px 0 0 0;color:#240101;background-color:#fff;border-radius:10px;top:calc(50vh - 132px);left:calc(50vw - 150px);position:relative}.mask .inner .p{margin:15px 20px}.mask .inner .p .number{display:inline-block;width:20px;height:20px;line-height:20px;text-align:center;font-size:14px;color:#300202;font-weight:700}.mask .inner .btns{display:flex;justify-content:space-around;margin-top:50px}.mask .inner .btns view{width:122px;height:40px;text-align:center;line-height:40px;font-size:15px;border-radius:20px}.mask .inner .btns view:nth-child(1){background-color:#fffcd5}.mask .inner .btns view:nth-child(2){background-color:#ffe227}.mask .inner .close{width:26px;height:26px;position:absolute;right:-13px;top:-13px;border-radius:50%}.empty{width:100vw;height:100vh;position:fixed;z-index:2;top:0;left:0;background-color:#fff;display:flex;flex-direction:column;justify-content:center;align-items:center;color:#240101;font-size:15px}.empty image{width:163px;height:105px;margin-bottom:10px}