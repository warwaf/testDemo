.collect-guide {
  position: fixed;
  top: 52px;
  right: 18px;
  z-index: 1;
  width: 336px;
  height: 59px;
  line-height: 62px;
  text-align: center;
  background-size: contain;
  background-repeat: no-repeat
}

.box {
  background-color: #ffeb4f
}

.banner {
  width: 100%;
  height: 151px;
  background: linear-gradient(180deg, rgba(255, 231, 70, .39), rgba(255, 255, 255, .54))
}

.search {
  position: absolute;
  left: 10px;
  width: 95px;
  height: 25px;
  padding: 0 15px;
  line-height: 25px;
  border-radius: 12px;
  background-color: rgba(255, 255, 255, .47);
  box-shadow: 0 0 13px rgba(0, 0, 0, .08);
  color: #240101;
  font-size: 12px;
  z-index: 1
}

.search image {
  position: relative;
  top: 2px;
  width: 12px;
  height: 12px;
  margin-right: 5px
}

.black_bar {
  width: 100vw;
  background-color: #000
}

.menu {
  width: 350px;
  height: 35px;
  border-radius: 5px;
  display: flex;
  margin: 10px auto 5px auto
}

.menu .menu_item {
  width: 50%;
  line-height: 35px;
  text-align: center;
  font-size: 15px;
  color: #222;
  background-color: #f0f4f8
}

.menu .menu_active {
  position: relative;
  background-color: #ffe329;
  color: #231815
}

.menu .menu_active .tangle {
  position: absolute;
  bottom: -6px;
  left: calc(50% - 3px);
  border-top: 6px solid #ffe329;
  border-left: 6px solid #fff;
  border-right: 6px solid #fff
}

.list-container {
  width: 98vw;
  display: flex;
  flex-wrap: wrap;
  padding: 0 1vw 80px 1vw
}

.list-container .guidance {
  width: 209px;
  position: fixed;
  top: 33px;
  left: 82px;
  z-index: 3
}

.room_item {
  width: 49vw;
  padding: 10px 0 0 0;
  border-bottom-left-radius: 16rpx;
  border-bottom-right-radius: 16rpx;
  position: relative;
  background-color: #fff
}

.room_item .upload-btn-animation {
  position: absolute;
  top: 100px;
  right: 80px
}

.room_item_0 {
  z-index: 3;
  border-radius: 10px 10px 0 0
}

.room_item_1 {
  z-index: 3;
  border-radius: 0 0 10px 10px
}

.room_item .beauty {
  width: 38px;
  height: 36px;
  position: absolute;
  left: -13px;
  top: -3px
}

.name {
  padding-left: 10px;
  margin-top: 5px;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
  font-size: 14px;
  font-family: PingFang-SC-Medium;
  color: #231815
}

.date {
  font-size: 12px;
  font-weight: 400;
  margin: 3px 0;
  color: #999;
  padding-left: 10px
}

.views {
  position: absolute;
  top: 73px;
  right: 15px;
  background-color: rgba(0, 0, 0, .5);
  color: #fff;
  padding: 0 8px;
  height: 17px;
  border-radius: 10px;
  display: flex;
  align-items: center;
  font-size: 12px
}

.views image {
  margin-right: 5px
}

.views image {
  width: 18px;
  height: 11px
}

.image_container {
  display: flex;
  justify-content: center
}

.image_container image {
  width: 168px;
  height: 84px;
  border-radius: 4px;
  box-shadow: 0 0 13px 0 rgba(0, 0, 0, .08);
  background-repeat: no-repeat;
  background-position: center center;
  background-size: cover;
  background-color: #f1f3f5;
  box-shadow: 0 -5px 10px rgba(0, 0, 0, .1)
}

.empty {
  display: flex;
  flex-direction: column;
  align-items: center;
  margin-top: 0
}

.bg_empty {
  width: 100%
}

.bg_empty image {
  width: 100%
}

.ewm {
  margin: 110rpx 0 0 0;
  width: 154rpx
}

.ewm image {
  width: 100%
}

.empty_word {
  font-size: 32rpx;
  margin: 20rpx 0 0 0;
  font-weight: 580;
  color: #240101
}

.empty_or image {
  margin: 40rpx 0;
  width: 50rpx
}

.search_empty {
  width: 48.2%;
  left: 10%;
  background-color: #fff;
  height: 30px;
  line-height: 30px;
  border-radius: 40px;
  font-size: 12px;
  display: flex;
  flex-direction: row;
  align-items: center;
  border: 1px solid #dbdbdb
}

.navigation {
  position: fixed;
  width: 100%
}

.navigationBox {
  width: 100%;
  height: 100rpx;
  top: 0;
  position: relative;
  z-index: 1;
  color: #222;
  font-size: 18px;
  transition: all linear .9s
}

.searchText_empty {
  width: 100%;
  padding-left: 15px
}

.searchText_empty input {
  width: 100%;
  margin-left: 30rpx
}

.erwmImg {
  width: 11%;
  height: 60rpx;
  text-align: center
}

.erwmImg image {
  width: 36rpx;
  height: 34rpx;
  margin-top: 14rpx
}

.erwmImg_empty {
  width: 11%;
  margin: 0 20rpx 0 0;
  height: 30px;
  text-align: center;
  vertical-align: middle
}

.erwmImg_empty image {
  width: 34rpx;
  height: 34rpx;
  margin-top: 14rpx
}

.empty_add {
  position: absolute;
  bottom: 100rpx
}

.empty_add image {
  width: 160rpx
}

.createImg {
  position: fixed;
  bottom: 100rpx;
  right: 10rpx;
  z-index: 3
}

.createImg .upload-btn-animation {
  position: absolute;
  top: 0;
  left: 2px
}

.createImg .text {
  width: 86px;
  height: 27px;
  position: absolute;
  top: -26px;
  left: 2px;
  animation-name: shock;
  animation-duration: 1s;
  animation-timing-function: linear;
  animation-delay: 0;
  animation-iteration-count: infinite
}

.createImg .plus {
  width: 66px;
  height: 66px;
  border-radius: 50%
}

.createImg button {
  color: #fff;
  background-color: transparent;
  border: none;
  height: 90px;
  padding: 0
}

.createImg button::after {
  border: none
}

.createImg .guidance {
  width: 215px;
  position: absolute;
  left: -204px;
  top: -84px
}

.active {
  position: fixed;
  left: 0;
  top: 0
}

.mask {
  width: 100vw;
  height: 100vh;
  position: fixed;
  top: 0;
  left: 0;
  background-color: rgba(0, 0, 0, .5);
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  z-index: 4
}

.mask>image {
  width: 280px;
  height: 360px;
  border-radius: 15px;
  margin-bottom: 15px
}

.mask .btn image {
  width: 48px;
  height: 48px
}

.coupon {
  position: fixed;
  bottom: 180px;
  right: 15px;
  width: 68px;
  height: 92px
}

.completed {
  height: 80px;
  line-height: 80px;
  width: 100%;
  text-align: center;
  color: #666;
  font-size: 12px
}

.swiper {
  width: 100vw;
  overflow: hidden;
  position: relative
}

.swiper image {
  width: 100%;
  height: 150px
}

.decoration-bar {
  position: absolute;
  width: 100%;
  height: 42px
}

.dots {
  width: 100vw;
  display: flex;
  justify-content: center;
  position: absolute;
  bottom: 10px
}

.dot {
  width: 6px;
  height: 6px;
  border-radius: 50%;
  margin-right: 10px;
  background-color: #eee
}

.active-dot {
  width: 11px;
  height: 6px;
  border-radius: 3px;
  background-color: #fd0 !important
}

.v-signin-mask {
  position: fixed;
  left: 0;
  top: 0;
  right: 0;
  bottom: 0;
  background: rgba(0, 0, 0, .5);
  z-index: 10
}

.v-signin-content {
  position: fixed;
  left: 0;
  top: 0;
  right: 0;
  bottom: 0;
  z-index: 11;
  display: flex;
  align-items: center;
  justify-content: center
}

.v-signin-date {
  width: 80vw;
  height: 68vh;
  background: #fff;
  border-radius: 15px;
  text-align: center;
  color: #231815;
  margin: 0 auto
}

.v-signin-date-top {
  margin-top: -22px;
  width: 90%
}

.v-signin-date .txt {
  color: #f50;
  font-size: 18px;
  display: inline-block
}

.v-signin-date-btn {
  width: 90%;
  height: 40px;
  line-height: 40px;
  background: #ffe228;
  border-radius: 23px;
  display: inline-block
}

.v-signin-result {
  position: relative
}

.v-signin-result-title {
  position: absolute;
  top: 40px;
  text-align: center;
  width: 100%;
  color: #231815;
  font-size: 15px;
  font-family: Arial-BoldMT
}

.v-signin-result-price {
  width: 100%;
  text-align: center;
  position: absolute;
  top: 120px;
  font-size: 65px;
  color: #231815;
  font-family: Arial-BoldMT
}

.v-date {
  display: flex;
  flex-wrap: wrap;
  margin: 15px
}

.v-date-item {
  width: 14.2%;
  display: inline-block
}

.v-date-item .items {
  border: 1px solid #dee1e9;
  border-radius: 4px;
  margin: 3px;
  height: 46px
}

.v-date-item .items.signin {
  border: 1px solid #fd0;
  background: #fd0
}

.album-type {
  width: 186px;
  height: 120px;
  background-size: cover;
  background-repeat: no-repeat;
  margin: 20px 0;
  padding: 10px 10px 10px 130px;
  border-radius: 10px;
  overflow: hidden;
  position: relative;
  left: 0;
  animation: insertFromLeft 1s 1 alternate
}

.album-type-l {
  animation: insertFromLeft 1s 1 alternate
}

.album-type-r {
  animation: insertFromRight 1s 1 alternate
}

@keyframes insertFromLeft {
  from {
    left: -100vw
  }

  to {
    left: 0
  }
}

@keyframes insertFromRight {
  from {
    left: 100vw
  }

  to {
    left: 0
  }
}

.album-type>view:nth-child(1) {
  color: #231815;
  font-size: 30px;
  margin: 10px 0;
  font-weight: 700
}

.album-type>view:nth-child(2) {
  font-size: 12px;
  color: #8b572a
}

.new {
  position: absolute;
  top: 260px;
  left: 0
}

.new .btn {
  width: 198px;
  height: 38px;
  text-align: center;
  line-height: 40px;
  border: 1px solid #bdbdbd;
  border-radius: 20px;
  margin: 10px auto
}

.welcome {
  width: 300px;
  height: 505px;
  background-color: #fff;
  border-radius: 10px;
  overflow: hidden
}

.welcome .top {
  width: 100%;
  height: 125px;
  padding-bottom: 20px;
  background-repeat: no-repeat;
  background-size: contain;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center
}

.welcome .top view {
  font-weight: 700;
  margin-top: 10px
}

.welcome .top image {
  width: 85px
}

.welcome .body {
  background-color: #fff;
  box-shadow: 0 -5px 7px rgba(0, 0, 0, .08);
  border-radius: 0 35px 0 0;
  position: relative;
  top: -25px
}

.welcome .body .txt {
  display: flex;
  justify-content: center;
  color: #9e6d00;
  font-size: 13px;
  font-weight: 700;
  padding: 15px 0 5px 0
}

.welcome .body .txt image {
  width: 26px;
  height: 12px;
  margin-left: 10px
}

.welcome .body .types {
  display: flex;
  flex-wrap: wrap
}

.welcome .body .types .type {
  margin: 10px 13px;
  width: 119px;
  height: 116px;
  padding: 5px 0;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-around;
  position: relative;
  border: 1px solid #e9e9e9
}

.welcome .body .types .type .t {
  width: 100%;
  color: #231815;
  font-size: 14px;
  padding-left: 12px
}

.welcome .body .types .type .d {
  width: 100%;
  color: #999;
  font-size: 11px;
  padding-left: 12px
}

.welcome .body .types .type .img {
  width: 106px;
  height: 67px
}

.welcome .body .types .type .label {
  width: 35px;
  height: 32px;
  position: absolute;
  top: -8px;
  left: -8px
}

.welcome .body .btn {
  width: 90%;
  height: 40px;
  line-height: 40px;
  background: #ffe228;
  border-radius: 23px;
  text-align: center;
  color: #231815;
  margin: 0 auto
}

.welcome .close-btn {
  width: 26px;
  height: 26px;
  position: absolute;
  right: -13px;
  top: -13px
}

.tool-case {
  position: fixed;
  right: 10px;
  bottom: 80px;
}

.tool-case .icon-ar {
  width: 61px;
  height: 61px;
}