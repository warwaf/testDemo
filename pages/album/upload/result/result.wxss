/* pages/album/upload/result/result.wxss */
.banner{
    width: 100%;
    height: 72px;
    background-repeat: no-repeat;
    background-size: cover;
    position: relative;
    margin-bottom: 30px;
}
.banner image{
    width: 69px;
    height: 69px;
    position: absolute;
    bottom: -10px;
    left: 50%;
    transform: translateX(-35px);
}
.header{
    width: 100%;
    text-align: center;
    margin: 20px 0;
}
.image-container{
    padding: 7px;
    display: flex;
    flex-flow: wrap;
}
.image-item{
    position: relative;
    margin: 7px;
    width: calc(25% - 14px);
    height: calc(0.25 * 100vw - 14px);
}
.image-item image{
    width: 100%;
    height: 100%;
}
.image-item .icon{
    position: absolute;
    right: 0;
    top: 0;
    width: 22px;
    height: 22px;
}
.notification view{
    color: #999;
    font-size: 13px;
    line-height: 20px;
    padding: 0 15px;
}
.notification>view:nth-child(1){
    color: #222222;
    font-size: 15px;
    font-weight: bold;
    margin: 10px 0;
}
.btn{
    width: 90%;
    height: 40px;
    line-height: 40px;
    text-align: center;
    border-radius: 20px;
    background: linear-gradient(#FFE74F,#FFE227);
    position: fixed;
    bottom: 10px;
    left: 5%;
}
.vouchers{
    position: fixed;
    z-index: 501;
}