/* pages/album/beauty/filterResult/filterResult.wxss */
.tab-header{
    height: 50px;
    display: flex;
    justify-content: space-between;
    align-items: center;
    padding: 0 10px 0 20px;
    background-color: #FFFCE3;
    border-radius: 10px 10px 0 0;
}
.info{
    width: 60%;
}
.tab-header .info .name{
    color: #222;
    margin: 5px 0;
    font-weight: bold;
    width: 90%;
    height: 22px;
    overflow: hidden;
    white-space: nowrap;
    text-overflow: ellipsis;
}
.tab-header .info .date{
    color: #999;
    font-size: 12px;
}
.empty{
    margin-top: 100px;
    display: flex;
    flex-direction: column;
    align-items: center;
}
.empty image{
    width: 100px;
}
.empty view{
    margin-top: 10px;
}