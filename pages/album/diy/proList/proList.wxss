/* pages/album/diy/proList/proList.wxss */

.container image{
    width: 100vw;
}
.btn-container{
    width: 100vw;
    padding: 15px 0;
    position: fixed;
    bottom: 0;
    background-color: #FFF;
    display: flex;
    flex-direction: column;
    align-items: center;
    color:#999;
    box-shadow: 0 -2px 5px #ddd;
}
.voucher{
    font-size: 14px;
    margin-bottom: 10px;
}
.btn{
    width: 94%;
    height: 40px;
    border-radius: 20px;
    background: linear-gradient(#FFE74F,#FFE227);
    color: #240101;
    line-height: 40px;
    text-align: center;
    font-weight: 600;
}