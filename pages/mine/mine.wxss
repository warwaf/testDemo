.tab1 {
    clear: both;
    text-align: center;
    position: absolute;
    top: 60rpx;
    width: 100%
}

.topBox {
    width: 90%;
    box-shadow: 0 0 26rpx 0 rgba(0, 0, 0, .11);
    display: flex;
    justify-content: center;
    align-content: center;
    flex-direction: column;
    margin-left: 5%;
    border-radius: 20rpx;
    background-color: #fff;
    position: relative;
}

.name {
    font-size: 34rpx;
    letter-spacing: 2rpx;
    height: 40rpx;
    line-height: 40rpx;
    margin: 20rpx 0;
    color: #222
}

.detail {
    font-size: 26rpx;
    margin: 10rpx 0 20rpx 0;
    color: #999;
    font-weight: 500;
    letter-spacing: 2rpx
}

.mine {
    border-radius: 8px
}

.his-name {
    font-size: 26rpx;
    letter-spacing: 2rpx;
    line-height: 38rpx;
    margin-bottom: 16rpx;
    color: #999;
    padding: 0 15px
}

.his-detail {
    font-size: 34rpx;
    margin: 10rpx 0 20rpx 0;
    color: #222;
    font-weight: 500;
    letter-spacing: 2rpx
}

.mine image {
    width: 150rpx;
    border-radius: 50%;
    overflow: hidden;
    margin-top: -60rpx;
    height: 150rpx;
    background-color: #fff;
    border: 6rpx solid #fff
}

.msgBox {
    display: flex;
    justify-content: center;
    flex-direction: row;
    margin: 16rpx 0
}

.msgTab {
    width: 33%;
    text-align: center;
    position: relative
}

.msgTabNum {
    font-size: 40rpx;
    height: 60rpx
}

.msgTabWord {
    font-size: 24rpx;
    color: #888
}

.v-tip {
    position: absolute;
    background: red;
    top: 0;
    right: 40px;
    border-radius: 8px;
    height: 8px;
    width: 8px;
    z-index: 100
}

.tab1 {
    top: 200rpx
}

.bg {
    width: 100%
}

.bg image {
    width: 100%;
    height: 92px
}

.topBox .mine {
    position: relative
}

.topBox .icon-vip {
    position: absolute;
    top: 0; right: 0;
    width: 60px;
    height: 60px;
    z-index: 10;
}

.topBox .mine .btn {
    position: absolute;
    right: 10px;
    top: 10px;
    height: 26px;
    width: 50px;
    line-height: 26px;
    border: 1px solid #ffe227;
    border-radius: 13px;
    font-size: 12px;
    color: #240101
}

.topBox .mine button:nth-child(1) {
    width: 76px;
    height: 76px;
    border-radius: 50%;
    box-shadow: 0 0 5px #ccc;
    margin: 10px auto;
    margin-top: -35px
}

.topBox .mine button image {
    width: 100%;
    height: 100%;
    margin: 0;
    border: none
}

.mine .name image {
    width: 14px;
    height: 14px;
    position: relative;
    top: 3px
}

.mine .detail {
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
    margin: 0 15px
}

.tab2 {
    width: 100%;
    display: flex;
    justify-content: center;
    align-content: center;
    flex-direction: row;
    margin-top: 90px
}

.tab2Box {
    width: 33%;
    margin: 20rpx 0;
    text-align: center
}

.tab2Box image {
    width: 138rpx;
    height: 138rpx;
    margin: 10rpx 0 0 0
}

.tab2Box view {
    font-size: 28rpx;
    color: #000;
    letter-spacing: 2rpx
}

.tab3 {
    width: 90%;
    margin: 0 0 20rpx 5%;
    padding-bottom: 50px;
    border-top: 1px solid #f2f2f2;
    display: flex;
    justify-content: center;
    flex-direction: column;
    align-content: center
}

.tab3Box {
    display: flex;
    align-content: center;
    flex-direction: row;
    height: 106rpx;
    line-height: 106rpx;
    border-bottom: 1rpx solid #f2f2f2;
    position: relative;
    align-items: center
}

.contact_btn{
    width: 100%;
    background-color: #fff;
    border: none;
    padding: 0;
    margin: 0;
    border-bottom: 1rpx solid #f2f2f2;
}
button::after{
    border: none;
}

.ml3Img {
    width: 40rpx;
    height: 42rpx;
    float: left;
    margin: 30rpx 0
}

.ml3Icon {
    width: 25rpx;
    position: absolute;
    right: 0;
    top: 0
}

.showMoney {
    position: absolute;
    right: 60rpx;
    color: #888;
    font-size: 26rpx
}

.ml3Icon image {
    margin: 36rpx 0;
    width: 16rpx;
    height: 34rpx
}

.ml3Word {
    font-size: 27rpx;
    color: #000;
    text-indent: 10rpx
}

.v-signin-mask {
    position: fixed;
    left: 0;
    top: 0;
    right: 0;
    bottom: 0;
    background: rgba(0, 0, 0, .5);
    z-index: 10
}

.v-signin-content {
    position: fixed;
    left: 0;
    top: 0;
    right: 0;
    bottom: 0;
    z-index: 11;
    display: flex;
    align-items: center;
    justify-content: center
}

.v-signin-date {
    width: 80vw;
    height: 68vh;
    background: #fff;
    border-radius: 15px;
    text-align: center;
    color: #231815;
    margin: 0 auto
}

.v-signin-date-top {
    margin-top: -22px;
    width: 90%
}

.v-signin-date .txt {
    color: #f50;
    font-size: 18px;
    display: inline-block
}

.v-signin-date-btn {
    width: 90%;
    height: 40px;
    line-height: 40px;
    background: #ffe228;
    border-radius: 23px;
    display: inline-block
}

.v-signin-result {
    position: relative
}

.v-signin-result-title {
    position: absolute;
    top: 40px;
    text-align: center;
    width: 100%;
    color: #231815;
    font-size: 15px;
    font-family: Arial-BoldMT
}

.v-signin-result-price {
    width: 100%;
    text-align: center;
    position: absolute;
    top: 120px;
    font-size: 65px;
    color: #231815;
    font-family: Arial-BoldMT
}

.v-date {
    display: flex;
    flex-wrap: wrap;
    margin: 15px
}

.v-date-item {
    width: 14.2%;
    display: inline-block
}

.v-date-item .items {
    border: 1px solid #dee1e9;
    border-radius: 4px;
    margin: 3px;
    height: 46px
}

.v-date-item .items.signin {
    border: 1px solid #fd0;
    background: #fd0
}

.authority-session.is-init {
    bottom: 90rpx;
}